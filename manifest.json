{
	"manifest_version": 2,

	"name": ":Procrastinotion",
	"description": "This extension is a timer.",
	"version": "0.0.0.1",

	"browser_action": {
		"default_icon": "eye-icon.png",
		"default_title": "Procrastinotion"
	},
	"background": {
		"scripts": ["pomodoroState.js", "breakState.js", "moment.js", "background.js"],
		"persistent": false
	},
	"permissions": [
		"notifications",
		"tabs"
	]
}